@model YothingoSprint1.Models.Employee



    @if (Model == null)
    {
        <div class="panel panel-danger">
            <div class="panel-body">No Supplier Selected!</div>
        </div>
    }
    else
    {
        <div class="panel panel-success">
            <div class="panel-body"> Full Name: @Model.Fullname</div>
            <div class="panel-body"> Job Title: @Model.JobTitle</div>
        </div>
    }
    </div>