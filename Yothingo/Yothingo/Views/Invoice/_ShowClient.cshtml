@model YothingoSprint1.Models.Supplier

    @if (Model == null)
    {
        <div class="panel panel-danger">
            <div class="panel-body">No Client Selected!</div>
        </div>
    }
    else
    {
        <div class="panel panel-success">
            <div class="panel-body"> Name: @Model.Name</div>
            <div class="panel-body"> Address: @Model.Address</div>

        </div>
    }