@model YothingoSprint1.Models.Request

@if (Model == null)
{
    <div class="panel panel-danger">
        <div class="panel-body">No Client Selected!</div>
    </div>
}
else
{
    <div class="panel panel-success">
        <div class="panel-body"> Name: @Model.RequestNo</div>
        <div class="panel-body"> Address: @Model.UserId</div>

    </div>
}